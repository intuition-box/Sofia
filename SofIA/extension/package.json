{
  "name": "sofia",
  "displayName": "SofIA",
  "version": "0.0.1",
  "description": "Data tracking and wallet connexion",
  "author": "me",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package",
    "proxy": "bun sofia-proxy/server.ts"
  },
  "dependencies": {
    "@0xintuition/1ui": "^0.4.1",
    "@0xintuition/protocol": "1.0.0-alpha.2",
    "@plasmohq/storage": "^1.15.0",
    "@privy-io/react-auth": "^2.20.0",
    "@privy-io/wagmi": "^1.0.5",
    "@tanstack/react-query": "^5.0.0",
    "@types/ws": "^8.18.1",
    "@wagmi/connectors": "^5.9.0",
    "ai": "^4.3.19",
    "cors": "^2.8.5",
    "evt": "^2.5.9",
    "express": "^5.1.0",
    "lucide-react": "^0.525.0",
    "node-fetch": "^3.3.2",
    "plasmo": "0.90.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "^7.7.1",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "umami": "^2.10.0",
    "viem": "^2.33.1",
    "wagmi": "^2.16.0",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@types/chrome": "0.0.258",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/node": "20.11.5",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "@types/react-router-dom": "^5.3.3",
    "prettier": "3.2.4",
    "typescript": "5.3.3"
  },
  "manifest": {
    "permissions": [
      "storage",
      "history",
      "tabs",
      "activeTab",
      "alarms",
      "sidePanel"
    ],
    "host_permissions": [
      "<all_urls>",
      "http://127.0.0.1:8080/*"
    ],
    "side_panel": {
      "default_path": "sidepanel.html"
    },
    "action": {},
    "web_accessible_resources": [
      {
        "resources": [
          "public/spline-background.webm",
          "assets/*",
          "components/ui/*.png",
          "components/ui/quick_action/*.svg"
        ],
        "matches": [
          "<all_urls>"
        ]
      }
    ]
  }
}
