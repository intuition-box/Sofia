  "description": "Sofia Fee Proxy Contract for Intuition MultiVault",
  "scripts": {
    "compile": "hardhat compile",
    "test": "hardhat test",
    "test:v": "hardhat test --verbose",
    "test:gas": "REPORT_GAS=true hardhat test",
    "coverage": "hardhat coverage",
